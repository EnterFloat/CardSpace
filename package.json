    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "gulp-sass": "^4.1.0",
    "jodit-react": "^1.0.70",
    "node-sass": "^4.14.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-app-polyfill": "^1.0.6",
